if res[1] <> "Files Equal!" then
  filterSimulationResults("PNlib.Examples.ConTest.SinglePC_res.mat", "../../ReferenceFiles/PNlib.Examples.ConTest.SinglePC_minimal.mat", vars, 0); getErrorString();
end if;

